@Controller
public class ControllerB {
	// DispatherServlet 
	//  org.springframework.web.servlet
	
		@RequestMapping("b1")
		public void a1() {
			System.out.println("a1()...... ");
		}
		
		@RequestMapping("b2")
		public void a2() {
			System.out.println("a2()...... ");
		}
}
