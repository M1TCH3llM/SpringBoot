package com.synergisticsit.util;

import java.util.Arrays;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.CommandLineRunner;
import org.springframework.boot.SpringApplication;

import com.synergisticit.domain.Employee;
import com.synergisticit.service.EmployeeService;

public class CommandLineRunnerImpl implements CommandLineRunner {
	
	@Autowired EmployeeService employeeServiced;


	@Override
	public void run(String... args) throws Exception {
		// TODO Auto-generated method stub
		List<Employee> employees = Arrays.asList(
			new Employee(1, "Afraz", "CEO",  15556),
			new Employee(2, "Jeremaih", "Pregrammer",  12987),
			new Employee(3, "Tejas", "Sr. Pregrammer",  15556),
			new Employee(4, "Himavan", "Tester",  15777),
			new Employee(5, "Mike", "Pregrammer",  15556),
			new Employee(1, "Mitch", "CTO",  20000)

);

		employees.forEach(e -> employeeService.save(e));
	}

 
}
